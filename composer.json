{
    "name": "isaacdanielreyna/mediacatalog",
    "description": "A SilverStripe module for media cataloging",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/isaacdanielreyna/silverstripe-mediacatalog",
    "keywords": ["silverstripe","media catalog"],
	"authors": [
		{
			"name": "Isaac Daniel Reyna",
			"email": "silverstripe@isaacreyna.com"
		}
	],
    "license": "none",
    "require": {
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "IsaacDanielReyna\\MediaCatalog\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "expose": [
            "css",
            "javascript",
            "images"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
